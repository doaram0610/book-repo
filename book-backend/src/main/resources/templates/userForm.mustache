{{>./header}}

<h3>회원 가입 화면</h3>

<form>
  <div class="mb-3 mt-3">
    <label for="userId" class="form-label">아이디:</label>
    <input type="text" class="form-control" id="userId" placeholder="Enter userId" name="userId">
  </div>
  <div class="mb-3">
    <label for="userPwd" class="form-label">Password:</label>
    <input type="password" class="form-control" id="userPwd" placeholder="Enter password" name="userPwd">
  </div>
  <div class="mb-3">
    <label for="userName" class="form-label">이름:</label>
    <input type="text" class="form-control" id="userName" placeholder="Enter userName" name="userName">
  </div>
  <div class="mb-3">
    <label for="role" class="form-label">권한:</label>
    <input type="text" class="form-control" id="role" placeholder="Enter role" name="role">
  </div>
  <button type="button" class="btn btn-primary" id="btn-submit">로그인</button>
</form>

{{>./footer}}
